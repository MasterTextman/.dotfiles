! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #000000, #00b1ff, #000000
rofi.color-normal: #000000, #00cbff, #202020, #00789e, #eee8d5
rofi.color-active: #000000, #ffa500, #202020, #966100, #fdf6e3
rofi.color-urgent: #000000, #dc322f, #202020, #dc322f, #fdf6e3

!rofi.modi: window,run,ssh,drun,combi
rofi.modi: combi,run
rofi.show: combi,run
rofi.combi-modi:window,drun
rofi.font: Terminess Powerline 9

urxvt*font: xft:Terminess Powerline:pixelsize=10
urxvt*boldFont: xft:Terminess Powerline:bold:pixelsize=10
urxvt*scrollBar: false
urxvt*borderless: 1
urxvt*externalBorder: 0
urxvt*internalBorder: 0
URxvt*fading: 30
URxvt*geometry: 200x30
! Due to i3
URxvt.transparent: true
URxvt.shading:15

xterm*termName: xterm-256color

